{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "Verdict Distribution",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "sum by (verdict) (sna_evaluation_total)",
          "legendFormat": "{{verdict}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "title": "Evaluations Over Time",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0},
      "targets": [
        {
          "expr": "rate(sna_evaluation_total[$interval])",
          "legendFormat": "{{verdict}} / {{tier}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "title": "EAS Score",
      "type": "gauge",
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
      "targets": [
        {
          "expr": "sna_eas_current",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 0.3},
              {"color": "green", "value": 0.7}
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Execution Success Rate",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "sum(sna_execution_total{success=\"true\"}) / sum(sna_execution_total) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      }
    },
    {
      "title": "Execution Latency (p95)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 6, "y": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(sna_execution_latency_seconds_bucket[$interval]))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "title": "Pending Escalations",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
      "targets": [
        {
          "expr": "sna_escalation_pending_count",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "title": "Validation Results",
      "type": "barchart",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "sum by (status) (sna_validation_total)",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "title": "Notification Delivery",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "targets": [
        {
          "expr": "rate(sna_notification_total[$interval])",
          "legendFormat": "{{channel}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["sna", "network-autonomy"],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": [],
        "refresh": 1,
        "regex": ""
      },
      {
        "name": "interval",
        "type": "interval",
        "query": "1m,5m,15m,30m,1h,6h,12h,1d",
        "current": {"text": "5m", "value": "5m"},
        "auto": true,
        "auto_count": 30,
        "auto_min": "10s",
        "hide": 0
      }
    ]
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "title": "SNA Overview",
  "uid": "sna-overview",
  "version": 1
}
